{
  "compilerOptions": {
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "lib": [
      "dom"
    ],
    "baseUrl": ".",
    "noEmit": true
  },
  "include": [
    "./js/index.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
