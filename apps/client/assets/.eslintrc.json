{
  "parser": "@typescript-eslint/typescript-estree",
  "extends": [
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "sourceType": "module",
    "jsx": true
  },
  "env": {
    "es6": true,
    "node": true
  }
}
